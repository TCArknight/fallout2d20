      name="Head?"
      helptext="Specify whether this armor protects the head.">
      <it_tagcheck group="ArmorLoc" tag="Head"/>
      </inputthing>
    <inputthing
      name="Torso?"
      helptext="Specify whether this armor protects the torso.">
