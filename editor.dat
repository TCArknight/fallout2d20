  <editthing
    compset="Perk"
    name="Perk"
    prefix="prk"
    description="Each perk ..."
    summary="Defines a perk that can be bought.">
    <inputthing
      name="Is Ranked?"
      helptext="Is this a perk with multiple ranks?">
      <it_field field="prkIsRanked"/>
      </inputthing>
    <inputthing
      name="How Many Ranks?"
      helptext="Number of Ranks of perk">
      <it_field field="prkMaxRank"/>
      </inputthing>	
    <inputthing
      name="Level?"
      helptext="The Level that the perk can first be taken">
      <it_field field="prkTakeAtLevel"/>
      </inputthing>	
    <inputthing
      name="Levels per Rank"
      helptext="The numbers of levels between taking ranks">
      <it_field field="prkLevelIncrement"/>
      </inputthing>	
	  
	<inputthing>
      <it_groupref id="ChooseGroup"/>
      </inputthing>
    <inputthing>
      <it_groupref id="SourceInfo"/>
      </inputthing>
    </editthing>

