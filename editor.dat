<?xml version="1.0" encoding="UTF-8"?>

<!-- This file contains the definitions of how the Editor should handle all the
      various publicly accessible objects (i.e. component sets) within the game
      system. A separate "editthing" elemenet is defined for each component set,
      along with the appropriate details for how to configure each facet via
      "inputthing" elements.
-->

<document signature="Hero Lab Data">

  <inputthinggroup
    id="SourceInfo">
	<inputthing
      name="Source Info"
      helptext="Entries related to the source material are below..">
      <it_separator/>
      </inputthing>
    </inputthinggroup>
	  
  <inputthinggroup
    id="ModCategoriesWeapon">
	<inputthing
      name="Modifications"
      helptext="Entries related to any modifications are below..">
      <it_separator/>
      </inputthing>
    <inputthing
      name="Mod Categories Allowed"
      helptext="Specify the category for any Mods for this weapon.">
      <it_taglist group="ItemModCatAllowed" tag="?"/>
      </inputthing>
    <inputthing
      name="Mods Allowed"
      helptext="Specify any specific Mods for this weapon.">
      <it_taglist group="ItemModAllow" tag="?"/>
      </inputthing>
    </inputthinggroup>
	
  <inputthinggroup
    id="ChooseGroup">
    <inputthing
      name="Selections"
      helptext="User Selections">
      <it_separator/>
      </inputthing>
    <inputthing
      name="Menu #1 Label"
      helptext="Specify the label used to identify choices for display in the first menu. If empty, no first label is shown.">
      <it_field field="usrLabel1"/>
      </inputthing>
    <inputthing
      name="Menu #1 Source"
      helptext="Specify the source from which to retrieve choices for display in the first menu.">
      <it_tagpick group="ChooseSrc1" tag="?"/>
      </inputthing>
    <inputthing
      name="Menu #1 Tag Expression"
      helptext="Specify the tag expression used to identify choices for display in the first menu. If empty, no first menu is shown.">
      <it_field field="usrCandid1"/>
      </inputthing>
    <inputthing
      name="Menu #2 Label"
      helptext="Specify the label used to identify choices for display in the second menu. If empty, no second label is shown.">
      <it_field field="usrLabel2"/>
      </inputthing>
    <inputthing
      name="Menu #2 Source"
      helptext="Specify the source from which to retrieve choices for display in the second menu.">
      <it_tagpick group="ChooseSrc2" tag="?"/>
      </inputthing>
    <inputthing
      name="Menu #2 Tag Expression"
      helptext="Specify the tag expression used to identify choices for display in the second menu. If empty, no second menu is shown.">
      <it_field field="usrCandid2"/>
      </inputthing>
	</inputthinggroup>

  <inputthinggroup
    id="ResistanceRatings">
    <inputthing
      name="Resistance Ratings"
      helptext="DR Values for specific types.">
      <it_separator/>
      </inputthing>
    <inputthing
      name="Physical DR"
      helptext="Specify the Physical DR of the armor against attacks.">
      <it_field field="defPhysical"/>
      </inputthing>
    <inputthing
      name="Energy DR"
      helptext="Specify the Energy DR of the armor against attacks.">
      <it_field field="defEnergy"/>
      </inputthing>
    <inputthing
      name="Radiation DR"
      helptext="Specify the Radiation DR of the armor against attacks.">
      <it_field field="defRadiation"/>
      </inputthing>
	
    </inputthinggroup>

  <inputthinggroup
    id="ModResistRatings">
    <inputthing
      name="Resistance Ratings"
      helptext="DR Values for specific types.">
      <it_separator/>
      </inputthing>
    <inputthing
      name="Physical DR"
      helptext="Specify the modification to the Physical DR of the armor against attacks.">
      <it_field field="modDRPhysical"/>
      </inputthing>
    <inputthing
      name="Energy DR"
      helptext="Specify the modification to the Energy DR of the armor against attacks.">
      <it_field field="modDREnergy"/>
      </inputthing>
    <inputthing
      name="Radiation DR"
      helptext="Specify the modification to the Radiation DR of the armor against attacks.">
      <it_field field="modDRRadiation"/>
      </inputthing>
	
    </inputthinggroup>

  <inputthinggroup
    id="ArmorDetails">
    <inputthing
      name="Armor Type"
      helptext="Specify the type of armor this is.">
      <it_taglist group="ArmorType" tag="?"/>
      </inputthing>
    <inputthing
      name="Armor Locations"
      helptext="Specific areas that the Armor protects">
      <it_separator/>
      </inputthing>
    <inputthing
      name="Coverage"
      helptext="Specify what areas are protected by this armor.">
      <it_taglist group="ArmorLoc" tag="?"/>
      </inputthing>
    <inputthing
      name="Notes"
      helptext="Specific areas that the Armor protects">
      <it_separator/>
      </inputthing>
    <inputthing
      name="Armor Special"
      helptext="Specify any specific special tags for this weapon.">
      <it_taglist group="ArmorSpecial" tag="?"/>
      </inputthing>
    <inputthing
      name="Special Behaviors"
      helptext="Specify any special behaviors for the armor of which the user should be reminded.">
      <it_field field="defSpecial"/>
      </inputthing>
    </inputthinggroup>	
	
  <inputthinggroup
    id="PwrArmorDetails">
    <inputthing
      name="Power Armor Type"
