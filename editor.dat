-->
    </editthing>


  <!-- Origin editthing -->
  <editthing
    compset="Origin"
    name="Origins"
    prefix="spc"
    description="Origins have no special fields - only a name. If a race confers an ability,{br}create it as a special ability and bootstrap it from the race."
    summary="Defines an Origin that can be selected by a character.">
    <inputthing
      name="Hit Locations"
      helptext="What body parts for the origin (typically Head, Torso, LeftArm, RightArm, LeftLeg, RightLeg)">
      <it_separator/>
      </inputthing>
    <inputthing
      name="Body Locations"
      helptext="Specify what areas are available.">
      <it_taglist group="HitLocation" tag="?"/>
      </inputthing>
    <inputthing
      name="Traits"
      helptext="Trait-related Selections">
      <it_separator/>
      </inputthing>
    <inputthing
      name="Number of Traits"
      helptext="Specify the number of traits for the Origin.">
      <it_field field="orNumTraits"/>
      </inputthing>
	<inputthing
        name="Origin Trait"
        helptext="Select any traits for the origin.">
	  <it_bootlist compset="OriginTrait" />
      </inputthing>
    <inputthing
      name="Perk instead of a Trait?"
      helptext="Specify whether can select a Perk in place of an additional Trait.">
      <it_tagcheck group="User" tag="PerkForTrait"/>
      </inputthing>
    <inputthing
      name="Select Trait from a list?"
      helptext="Specify whether can select a Trait from a list.">
      <it_tagcheck group="User" tag="SelectTrait"/>
      </inputthing>
	<inputthing>
      <it_groupref id="ChooseGroup"/>
      </inputthing>
    <inputthing>
      <it_groupref id="SourceInfo"/>
      </inputthing>
    </editthing>

