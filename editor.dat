      <it_tagcheck group="ArmorLoc" tag="ArmLeft"/>
      </inputthing>
    <inputthing
      name="Right Arm?"
      helptext="Specify whether this armor protects the arms.">
      <it_tagcheck group="ArmorLoc" tag="ArmRight"/>
      </inputthing>
    <inputthing
      name="Left Leg?"
      helptext="Specify whether this armor protects the legs.">
      <it_tagcheck group="ArmorLoc" tag="LegLeft"/>
      </inputthing>
    <inputthing
      name="Right Leg?"
      helptext="Specify whether this armor protects the legs.">
      <it_tagcheck group="ArmorLoc" tag="LegRight"/>
      </inputthing>
    <inputthing
      name="Notes"
      helptext="Specific areas that the Armor protects">
      <it_separator/>
      </inputthing>
    <inputthing
      name="Armor Special"
      helptext="Specify any specific special tags for this weapon.">
      <it_taglist group="ArmorSpecial" tag="?"/>
      </inputthing>
    <inputthing
      name="Special Behaviors"
      helptext="Specify any special behaviors for the armor of which the user should be reminded.">
      <it_field field="defSpecial"/>
      </inputthing>
    </inputthinggroup>	
  <inputthinggroup
    id="ModArmorLoc">
	<inputthing
      name="Limited to Armor Location"
      helptext="Specific areas that the Armor protects">
      <it_separator/>
      </inputthing>
    <inputthing
      name="Head Only?"
      helptext="Specify whether this armor protects the head.">
      <it_tagcheck group="ItemModArmorLoc" tag="Head"/>
      </inputthing>
    <inputthing
      name="Torso Only?"
      helptext="Specify whether this armor protects the torso.">
      <it_tagcheck group="ItemModArmorLoc" tag="Torso"/>
      </inputthing>
    <inputthing
      name="Left Arm Only?"
      helptext="Specify whether this armor protects the arms.">
      <it_tagcheck group="ItemModArmorLoc" tag="ArmLeft"/>
      </inputthing>
    <inputthing
      name="Right Arm Only?"
      helptext="Specify whether this armor protects the arms.">
      <it_tagcheck group="ItemModArmorLoc" tag="ArmRight"/>
      </inputthing>
    <inputthing
      name="Left Leg Only?"
