      </inputthing>
    <inputthing
      name="Right Leg Only?"
      helptext="Specify whether this armor protects the legs.">
      <it_tagcheck group="ItemModArmorLoc" tag="LegRight"/>
      </inputthing>
    </inputthinggroup>
	
  <inputthinggroup
    id="BaseWeapon">
    <inputthing
      name="Weapon Type"
      helptext="Specify the type of weapon this is (this corresponds to the skill used).">
      <it_taglist group="WeaponType" tag="?"/>
      </inputthing>
    <inputthing
      name="Damage Rating"
      helptext="Specify the damage rating of the weapon.">
      <it_field field="wpDamage"/>
      </inputthing>
    <inputthing
      name="Effects"
      helptext="Specify any effects for this weapon.">
      <it_taglist group="Effect" tag="?"/>
      </inputthing>
    <inputthing
      name="Damage Type"
      helptext="Specify any effects for this weapon.">
      <it_taglist group="DamageType" tag="?"/>
      </inputthing>
    <inputthing
      name="Fire Rate"
      helptext="Specify the Fire Rate of the weapon.">
      <it_field field="wpFireRate"/>
      </inputthing>
    <inputthing
      name="Range"
      helptext="Specify any range for this weapon.">
      <it_taglist group="Range" tag="?"/>
      </inputthing>
    <inputthing
      name="Qualities"
      helptext="Specify any qualities for this weapon.">
      <it_taglist group="Quality" tag="?"/>
      </inputthing>
	
    </inputthinggroup>
	
  <inputthinggroup
    id="WeightCostLot">
	<inputthing
      name="Weight and Cost Info"
      helptext="Entries related to the source material are below..">
      <it_separator/>
      </inputthing>
    <inputthing
      name="Weight"
      helptext="Specify the weight of the item.">
      <it_field field="gearWeight"/>
      </inputthing>
    <inputthing
      name="Cost"
      helptext="Specify the standard cost of the item, with a value of 0 meaning it is free.">
      <it_field field="grCost"/>
      </inputthing>
    <inputthing
      name="Lot Cost"
      helptext="Specify the cost of a full 'lot' of the item, where the lot size is given on the right. The 'lot cost' overrides any individual cost given above.">
      <it_field field="grLotCost"/>
      </inputthing>
    <inputthing
      name="Rarity"
      helptext="Specify how common the item is on a scale of 0-10, with a value of 0 meaning it is very common.">
      <it_field field="grRarity"/>
      </inputthing>
    </inputthinggroup>
	
  <inputthinggroup
