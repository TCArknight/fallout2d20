  <thing id="armCasualHat" name="Casual Hat" description="A simple, lightweight hat, normally with a peak or brim to keep the sunlight out of the wearer’s eyes.\n\nSpecial: A character wearing a casual hat ignores any difficulty increases caused by extremely bright light." compset="Headgear">
    <fieldval field="grCost" value="15"/>
    <fieldval field="grRarity" value="1"/>
    <fieldval field="defSpecial" value="Ignore any difficulty increases caused by extremely bright light."/>
    <tag group="ItemModCatAllowed" tag="Clothing"/>
